<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="false"
	monitoring="off" dynamicConfig="true">

	 <diskStore path="cache/types/" />

	<defaultCache maxElementsInMemory="0" eternal="false"
		overflowToDisk="false" timeToIdleSeconds="30" timeToLiveSeconds="60">
	</defaultCache>

	<cache name="com.qp.dataCache" maxElementsInMemory="5000" eternal="true" overflowToDisk="true">
	</cache>
</ehcache>